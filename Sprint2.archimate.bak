  <folder name="Motivation" id="id-aa2331b97f8a417c9f5e3932112f569b" type="motivation">
    <element xsi:type="archimate:Stakeholder" name="Marisa" id="id-815f558fc29047798c9eee5c7093bcac"/>
    <element xsi:type="archimate:Driver" name="Atender melhor as expectivas dos clientes ao comprarem roupas" id="id-2c58f263bbfd4f1a8a559e2039e6d1f8"/>
    <element xsi:type="archimate:Value" name="Diminuir  a quantidade de devoluções por conta de tamanhos errados, que são cerca de 65% do motivo das trocas/devoluções" id="id-de63c4368db146fc8c04f25e376c9b1b"/>
    <element xsi:type="archimate:Value" name="Diminuir a quantidade de devolução por conta da expectativa não antendida, que representa 18% das trocas/devoluções" id="id-7245fb6d31844e16873861b30879aa25"/>
    <element xsi:type="archimate:Assessment" name="Permitir o produto atender as expectativas do cliente" id="id-f58ee4d7548e46faab571ea79142f904"/>
    <element xsi:type="archimate:Assessment" name="Fazer com que as medidas das roupas sejam adequadas pro cliente" id="id-591815db3cc642d3b615b359e668aa4d"/>
    <element xsi:type="archimate:Goal" name="Mostrar ao cliente a maneira como a roupa ficará nele" id="id-70f7b2d8e2764a439720cf6ffa8e22b8"/>
    <element xsi:type="archimate:Goal" name="Indicar o tamanho apropriado para a pessoa" id="id-a122ccf79bbf469593845cbf493282bb"/>
    <element xsi:type="archimate:Outcome" name="Realizar medições utilizando uma  IA com calibragem baseada em um objeto de tamanho fixo, como um cartão de crédito" id="id-7a5b826b90ad4d51933f391338287f30"/>
    <element xsi:type="archimate:Outcome" name="Com base em um modelo 3D, encaixar um preview da roupa na foto que a pessoa tirar" id="id-5c2263e2a99b457db22c7e372175f8c8"/>
    <element xsi:type="archimate:Requirement" name="Guardar as medidas da pessoa" id="id-bbe886b34b2e40b59556c1539ef70634"/>
    <element xsi:type="archimate:Requirement" name="Calibrar a IA com um objeto de tamanho fixo" id="id-de76a1faea294345a84ae8f9554343f3"/>
    <element xsi:type="archimate:Requirement" name="Permitir adicionar novos modelos de roupa" id="id-7a191f18bbc84bd583d783944b4546be"/>
    <element xsi:type="archimate:Requirement" name="Ter um banco de dados para guardar as roupas ja adicionadas" id="id-33b0ff5f36d04539bfcc6ba0e2591794"/>
    <element xsi:type="archimate:Constraint" name="Manter uma base de dados que guarda as medidas dos clientes e o resultado das pesquisas de satisfação, para que a uma futura IA seja calibrada, e então possa ser ainda mais precisa na indicação de tamanhos" id="id-639d4a3210cf465a959eeec4fe7ecb4a"/>
    <element xsi:type="archimate:Requirement" name="Mandar uma pesquisa de satisfação no email após a entrega do produto" id="id-41cacd5020414223bbf1e167b91982e4"/>
    <element xsi:type="archimate:Constraint" name="Realizar uma métrica das roupas mais acessadas e as mais compradas, pra assim existir um controle e poder recomendar produtos para outros clientes" id="id-d02a483c72b14e6593a0d2376bc67300"/>
    <element xsi:type="archimate:Principle" name="A pesquisa não é obrigatória" id="id-4f8d77eb9c9c4e2cae30245adf36d8e9"/>
    <element xsi:type="archimate:Principle" name="A interface deve ter uso intuitivo, e uma aba de ajuda, para que seja fácil pessoas mais velhas entenderem a plataforma" id="id-09c251a32f5d4777a1e5b9b9a847fae8"/>
  </folder>
